.mui-search{font-size: 0.3rem;font-weight: 600;}
.mui-search img{width: 30px;margin-bottom: -6px;margin-right:7px;}
form{margin-top: 5px;}
.top-header{height: 60px; padding: 16px 15px;}
.mui-views,
.mui-view,
.mui-pages,
.mui-page,
.mui-page-content {
	position: absolute;
	left: 0;
	right: 0;
	top: 0;
	bottom: 0;
	width: 100%;
	height: 100%;
}

.mui-pages {
	top: 50px;
	height: auto;
}

.mui-bar-nav .mui-btn {
	color: #fff;
	font-size: 0.8125em;
}

.mui-pages .mui-page {
	display: block;
}

#searchinput {
	text-align: left;
	border-radius: 2px;
	background-color: #fff;
	border: 1px solid #EBEBEB;
	height: 28px;
	padding: 0 5px;
	font-size: 15px;
}

.mui-input-row label {
	font-size: 0.3rem;
	width: 18%;
	line-height: 1.3;
	padding:7px 2px 11px 15px;
}
.mui-input-row label img{
	width: 30px;
    margin-bottom: -10px;
    margin-right:4px;
}
.mui-input-row label~input {
	width: 82%;
	font-size: 0.3rem;
}

.mui-search .mui-placeholder {
	text-align: left;
	border-radius: 2px;
	background-color: #fff;
	border: 1px solid #EBEBEB;
	height: 28px;
	padding: 0 5px;
	font-size: 15px;
	line-height: 25px;
}

.mui-search .mui-placeholder .mui-icon {
	font-size: 20px;
	color: #999;
	float: right;
}

.mui-search.mui-active:before {
	display: none;
}

.mui-icon {
	line-height: 1.2;
}
input[type=search]{
	height: 28px;
}