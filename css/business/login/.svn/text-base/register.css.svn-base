/*注册*/
html, body {
    height: 100%;
    margin: 0px;
    padding: 0px;
    overflow: hidden;
    -webkit-touch-callout: none;
    -webkit-user-select: none;
}
.mui-input-row label{padding: 0.5rem 0 0 0;width: 27%;}
.mui-input-row label .img-mar{margin-bottom: -0.4rem;margin-right: 0.3rem;}
input, select, textarea {font-size: 14px;}
.mui-input-group:after {background-color:rgba(255,255,255,0.1)}
.mui-input-row{margin: 0 0.6rem;height: 48px !important;}
.mui-input-group .mui-input-row:after {left:0}
.mui-input-row label~input{float: left;width: 46%;}
.verification{width: 5.6em;height: 2.0625em;line-height:2.0625em;margin: 10px 0;right: 2px; float:right;padding: 0;border: 1px #f26b44 solid;font-size:1em;border-radius: 6px;}
.chk_1 {
    display: none;
}
.chk_1:checked+ label {
    border: 1px solid #8f8f94;
    color: #243441;
    font-family: Muiicons;
    border-radius: 4px;
}
.chk_1+ label {
    background-color: #FFF;
    border: 1px solid #8f8f94;
    box-shadow: 0 1px 2px rgba(0, 0, 0, 0.05), inset 0px -15px 10px -12px rgba(0, 0, 0, 0.05);
    padding: 6px;
    display: inline-block;
    position: relative;
    margin-right: 5px;
    vertical-align: middle;
    border-radius: 4px;
}
.chk_1:checked+ label:after {
    content: '\e472';
    position: absolute;
    top: 0px;
    left: 0px;
    color: #8f8f94;
    width: 100%;
    text-align: center;
    font-size: 28px;
    padding: 1px;
    vertical-align: text-top;
    margin-left: -9px;
    margin-top: -5px;
}

