html, body {
    height: 100%;
    margin: 0px;
    padding: 0px;
    overflow: hidden;
    -webkit-touch-callout: none;
    -webkit-user-select: none;
}
.mui-table-view-cell:after{
	right: 15px;
}
.mui-table-view:after{
	left: 15px;
	right: 15px;
}
.mui-content,html{
	background-color: #FFFFFF;
}
li{
	/*height: 1.1rem;*/
