/*投诉*/
.textarea-body{min-height: 180px;}
.textarea-body textarea{color: #999999;font-size: 14px;padding:5px 4px;}
.textarea-body p{margin-top: -15px;float: right;margin-right: 10px}
.mui-card-0{box-shadow:0 0 0;margin: 0 10px 10px 10px;}
/*选择投诉问题*/
.mui-card-1{margin: 0 ;}
.mui-input-group .mui-input-row:after{left:0}
.mui-content>.mui-table-view:first-child {margin-top:0px;}
.mui-checkbox input[type=checkbox]:checked:before, .mui-radio input[type=radio]:checked:before {color: #fcb400;}
.mui-table-view .mui-media-object{ max-width: 28px;height: 28px;margin-top: 4px;}
.mui-radio.mui-left input[type=radio] {left: 18px;margin-top: 4px;}			
.mui-checkbox input[type=checkbox]:before, .mui-radio input[type=radio]:before{font-size: 22px;}
.mui-radio input[type=radio]:checked:before {content: '\e442';}	